<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="scenePane" prefHeight="398.0" prefWidth="608.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jfx.cmnbtwcontroller.WelcomeController">
   <children>
      <ImageView fitHeight="406.0" fitWidth="608.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@5153829.jpg" />
         </image>
      </ImageView>
      <Label fx:id="welcomeLabel" layoutX="14.0" layoutY="14.0" prefHeight="50.0" prefWidth="346.0" text="Welcome! ">
         <font>
            <Font name="Lucida Console" size="29.0" />
         </font>
      </Label>
      <Button fx:id="logout" layoutX="490.0" layoutY="339.0" mnemonicParsing="false" onAction="#logout" text="Logout">
         <font>
            <Font name="Lucida Console" size="13.0" />
         </font>
      </Button>
      <Label layoutX="354.0" layoutY="342.0" text="Want to logout?">
         <font>
            <Font name="Lucida Console" size="14.0" />
         </font>
      </Label>
      <ImageView fitHeight="84.0" fitWidth="79.0" layoutX="177.0" layoutY="64.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@blue-archive-alisu.gif" />
         </image>
      </ImageView>
      <Button fx:id="imageSetBtn" layoutX="24.0" layoutY="144.0" mnemonicParsing="false" onAction="#switchToImageShow" prefHeight="33.0" prefWidth="216.0" text="Continue to Image set?">
         <font>
            <Font name="Lucida Console" size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>

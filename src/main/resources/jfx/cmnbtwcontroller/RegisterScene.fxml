<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jfx.cmnbtwcontroller.RegisterController">
   <children>
      <ImageView fitHeight="402.0" fitWidth="602.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@5153829.jpg" />
         </image>
      </ImageView>
      <Label fx:id="registerTitle" layoutY="28.0" prefHeight="30.0" prefWidth="601.0" text="Create an account">
         <font>
            <Font name="Lucida Console" size="27.0" />
         </font>
      </Label>
      <Label layoutX="140.0" layoutY="148.0" text="Username:">
         <font>
            <Font name="Lucida Console" size="16.0" />
         </font>
      </Label>
      <TextField fx:id="usernameLabel" layoutX="234.0" layoutY="145.0" prefHeight="26.0" prefWidth="187.0" />
      <PasswordField fx:id="passwordLabel" layoutX="234.0" layoutY="188.0" prefHeight="26.0" prefWidth="187.0" />
      <Label layoutX="140.0" layoutY="191.0" text="Password:">
         <font>
            <Font name="Lucida Console" size="16.0" />
         </font>
      </Label>
      <PasswordField fx:id="rePasswordLabel" layoutX="234.0" layoutY="231.0" prefHeight="26.0" prefWidth="187.0" />
      <Label layoutX="53.0" layoutY="234.0" text="Re-enter password:">
         <font>
            <Font name="Lucida Console" size="16.0" />
         </font>
      </Label>
      <Button fx:id="registerBtn" layoutX="352.0" layoutY="355.0" mnemonicParsing="false" onAction="#register" text="Register!">
         <font>
            <Font name=".VnArial" size="13.0" />
         </font>
      </Button>
      <Label fx:id="messageLabel" layoutX="62.0" layoutY="358.0" prefHeight="18.0" prefWidth="265.0">
         <font>
            <Font name="Lucida Console" size="15.0" />
         </font>
      </Label>
      <ImageView fitHeight="99.0" fitWidth="121.0" layoutX="27.0" layoutY="153.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@aris-dancing-arisu-blue-archive.gif" />
         </image>
      </ImageView>
      <Label layoutX="130.0" layoutY="106.0" text="Full name:">
         <font>
            <Font name="Lucida Console" size="16.0" />
         </font>
      </Label>
      <TextField fx:id="fullNameField" layoutX="234.0" layoutY="102.0" prefHeight="26.0" prefWidth="187.0" />
      <RadioButton fx:id="Male" layoutX="234.0" layoutY="276.0" mnemonicParsing="false" selected="true" text="Male">
         <toggleGroup>
            <ToggleGroup fx:id="Gender" />
         </toggleGroup></RadioButton>
      <RadioButton fx:id="Female" layoutX="298.0" layoutY="276.0" mnemonicParsing="false" text="Female" toggleGroup="$Gender" />
      <RadioButton fx:id="Unknown" layoutX="372.0" layoutY="276.0" mnemonicParsing="false" text="Unknown" toggleGroup="$Gender" />
      <Label layoutX="156.0" layoutY="275.0" text="Gender:">
         <font>
            <Font name="Lucida Console" size="16.0" />
         </font>
      </Label>
      <DatePicker fx:id="dateOfBirthField" layoutX="234.0" layoutY="299.0" onAction="#getDateOfBirth" prefHeight="26.0" prefWidth="187.0" />
      <Label layoutX="88.0" layoutY="303.0" text="Date of birth:">
         <font>
            <Font name="Lucida Console" size="16.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
